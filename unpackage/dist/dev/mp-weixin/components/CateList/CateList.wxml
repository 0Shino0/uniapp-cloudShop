<view class="cateListContainer"><swiper indicator-dots="{{true}}" autoplay="{{true}}" indicator-color="green" indicator-active-color="pink"><block wx:for="{{cateObj.category.bannerUrlList}}" wx:for-item="swiperItem" wx:for-index="index" wx:key="index"><swiper-item><image src="{{swiperItem}}" mode></image></swiper-item></block></swiper><text class="title">{{cateObj.category.name}}</text><text class="desc">{{cateObj.category.frontDesc}}</text><view class="shopList"><block wx:for="{{cateObj.itemList}}" wx:for-item="shopItem" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['cateObj.itemList','id',shopItem.id]]]]]]]}}" class="shopItem" bindtap="__e"><image class="shopImg" src="{{shopItem.listPicUrl}}" mode></image><view class="shopInfo common">{{shopItem.name}}</view><view class="shopPrice common">{{shopItem.retailPrice}}</view></view></block></view></view>